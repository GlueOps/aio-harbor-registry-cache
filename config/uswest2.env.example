# US West 2 region environment configuration template
# Copy this file to config/uswest2.env and customize for your US West 2 deployment

# Harbor configuration for US West 2
export HARBOR_HOSTNAME=harbor-usw2.yourdomain.com
export HARBOR_DATA_VOLUME_PATH=/opt/harbor/data
export HARBOR_NGINX_CERT_LOCATION=/etc/ssl/certs/harbor-usw2.crt
export HARBOR_NGINX_KEY_LOCATION=/etc/ssl/private/harbor-usw2.key

# OpenTofu configuration for US West 2
export HARBOR_URL="https://harbor-usw2.yourdomain.com"
export HARBOR_PASSWORD="CHANGE_ME_SECURE_PASSWORD"

# US West 2 OIDC configuration
export TF_VAR_GOOGLE_OIDC_CLIENT_ID="your-usw2-client-id.apps.googleusercontent.com"
export TF_VAR_GOOGLE_OIDC_CLIENT_SECRET="your-usw2-client-secret"

# Certificate generation - typically false for production (use real certs)
export CREATE_LOCAL_CERTS=false
